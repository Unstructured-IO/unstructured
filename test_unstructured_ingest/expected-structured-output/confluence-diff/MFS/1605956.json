        "version": "1"
      },
      "emphasized_text_contents": [
        "Introduce the team"
      ],
      "emphasized_text_tags": [
        "b"
      ],
      "filetype": "text/html",
      "languages": [
        "eng"
      ]
    },
    "text": "Introduce the team",
    "type": "Title"
  },
  {
    "element_id": "29256b0f8b56f95a223d5f5c520bb1a4",
    "metadata": {
      "data_source": {
        "date_created": "2023-07-09T12:54:45.288000",
        "date_modified": "2023-07-09T12:54:45.288000",
        "record_locator": {
          "page_id": "1605956",
