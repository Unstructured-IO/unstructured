    "text": "3.1 Layout Detection Models"
  },
  {
    "type": "NarrativeText",
    "element_id": "6308564d51ecc38117d56ca4d886e7b7",
    "metadata": {
      "data_source": {
        "url": "/home/runner/work/unstructured/unstructured/test_unstructured_ingest/example-docs/layout-parser-paper-with-table.jpg",
        "permissions_data": [
