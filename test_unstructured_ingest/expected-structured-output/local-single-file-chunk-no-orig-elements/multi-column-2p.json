    "element_id": "4edd1cc01ad5339b3fc211258d615bfe",
    "text": "Introduction",
    "metadata": {
      "data_source": {
        "record_locator": {
          "path": "/home/runner/work/unstructured/unstructured/example-docs/multi-column-2p.pdf"
        },
